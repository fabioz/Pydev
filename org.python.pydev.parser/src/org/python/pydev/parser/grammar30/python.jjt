|   ( t=<SINGLE_BSTRING> { jjtThis.setImage(makeString(t.image, 1)); } {} )#Binary
|   ( t=<SINGLE_BSTRING2> { jjtThis.setImage(makeString(t.image, 1)); } {} )#Binary
|   ( t=<TRIPLE_BSTRING> { jjtThis.setImage(makeString(t.image, 3)); } {} )#Binary
|   ( t=<TRIPLE_BSTRING2> { jjtThis.setImage(makeString(t.image, 3)); } {} )#Binary
}

